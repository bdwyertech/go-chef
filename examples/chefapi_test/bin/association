#!/bin/bash

# Group testing

BASE=$(dirname $0)

. ${BASE}/setup
. ${BASE}/creds
go run ${BASE}/../cmd/association/association_setup.go ${CHEFUSER} ${KEYFILE} ${CHEFGLOBALURL} ${SSLBYPASS}
go run ${BASE}/../cmd/association/association.go ${CHEFUSER} ${KEYFILE} ${CHEFORGANIZATIONURL} ${SSLBYPASS} 
go run ${BASE}/../cmd/association/association_cleanup.go ${CHEFUSER} ${KEYFILE} ${CHEFGLOBALURL} ${SSLBYPASS}
